::-webkit-scrollbar {
  display: none;
}

.title {
  display: line-block;
  font-size: 30rpx;
  text-align: center;
}

button {
  padding-left: 0;
  padding-right: 0;
}

.avatar {
  display: block;
  width: 120rpx;
  height: 120rpx;
  z-index: 1000;
}

.header {
  background: #fff;
  display: block;
  height: 90rpx;
  border-bottom: 8rpx solid #ebebeb;
  position: fixed;
  top:0;
  width: 100%;
  z-index: 1000;
}

.header view {
  display: inline-block;
  min-width: 33.33%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}

.sumbtn {
  width: 90%;
  height: 94rpx;
  line-height: 94rpx;
  border-radius:0;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  background: #de7c21;
  margin: 0 auto;
}

.price {
