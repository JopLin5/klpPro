}

.fix {
  position: fixed;
}

.rowline {
  width: 100%;
  height: 1rpx;
  background: #c0c0c0;
}

.right-arrow {
  display: inline-block;
  position: relative;
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}

.right-arrow::after {
  display: inline-block;
  content: " ";
  height: 18rpx;
  width: 18rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #c7c7cc;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  right: 6rpx;
  margin-top: -9rpx;
}

.leadered-item {
  display: flex;
  line-height: 150rpx;
  height: 150rpx;
  padding-left: 20rpx;
  border-bottom: 8rpx solid #ebebeb;
}

.leadered-item .item-image {
  width: 110rpx;
  height: 110rpx;
  margin-top: 25rpx;
  border-radius: 10rpx;
  overflow: hidden;
  border: 1px solid #fff;
  margin-right: 20rpx;
}

.leadered-item .item-content {
  display: flex;
  flex: 1;
}

.leadered-item .item-image image {
  width: 110rpx;
  height: 110rpx;
}

.leadered-item .item-name {
  flex: 1;
}

.leadered-item .item-arrow {
  margin-top: 57rpx;
}

